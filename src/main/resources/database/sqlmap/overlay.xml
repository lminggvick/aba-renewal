<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.abacorp.aba.model.Overlay">

    <!-- Mapper -->
    <resultMap id="Overlay" type="Overlay" />

    <select id="findSiOverlays" resultMap="Overlay">
        SELECT * FROM overlays WHERE weight = 1;
    </select>

    <select id="findOverlayByWeight" resultMap="Overlay" parameterType="String">
        SELECT * FROM overlays WHERE weight = #{weight};
    </select>
</mapper>
